{
    "name": "andrewprofile/plugin",
    "description": "Plugin for eCommerce",
    "license": "proprietary",
    "type": "project",
    "repositories": [
        {
            "type": "artifact",
            "url": "../artifact/"
        }
    ],
    "require": {
        "php": "~7.1",
        "andrewprofile/micro-kernel": "1.0.0-dev",
        "andrewprofile/http-client": "1.0.0-dev",
        "psr/http-message": "@stable",
        "ext-json": "*",
        "ext-curl": "*"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "jakub-onderka/php-parallel-lint": "@stable",
        "phpstan/phpstan": "@stable",
        "phpmd/phpmd" : "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "infection/infection": "^0.13.6",
        "phpunit/phpunit": "^7",
        "sebastian/phpcpd": "^4.1",
        "phploc/phploc": "^4.0",
        "phan/phan": "^2.3",
        "vimeo/psalm": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "DNA\\Plugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DNA\\Plugin\\": "tests"
        }
    }
}
